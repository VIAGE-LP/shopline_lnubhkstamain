<script>
    $(document).ready(function () {
        /*手機切換漢堡選單*/
        $(".menu").hide();
        $('#nav-icon').click(function () {
            $(this).toggleClass('open');
            $(".menu").slideToggle("normal");
        });
        $(".menu > ul > div > li > a.scrollLink").click(function (event) {
            $('#nav-icon').toggleClass('open');
            $(".menu").slideToggle("normal");
        });

        /*畫面跳到指定位置*/
        $("a.scrollLink").click(function (event) {
            event.preventDefault();
            $("html, body").animate({
                scrollTop: ($($(this).attr("href")).offset().top - 0)
            }, 400);
        });
    });
</script>

<script>
    function toggleContent(e) {
        var Content = document.querySelector(`${e}`);

        if (Content.style.maxHeight) {
            Content.style.maxHeight = null;
        } else {
            Content.style.maxHeight = Content.scrollHeight + "px";
        }
    }
</script>
<script src="https://viage-lp.github.io/shopline_lnubhksta/js/bootstrap_custom.js"></script>
<!-- <script src="js/bootstrap_custom.js"></script> -->
<script>
    $(document).ready(function () {
        let items = document.querySelectorAll(".carousel .carousel-item");

        items.forEach((el) => {
            const minPerSlide = 3
            let next = el.nextElementSibling
            for (var i = 1; i < minPerSlide; i++) {
                if (!next) {
                    // wrap carousel by using first child
                    next = items[0]
                }
                let cloneChild = next.cloneNode(true)
                el.appendChild(cloneChild.children[0])
                next = next.nextElementSibling
            }

        })
    });
</script>

